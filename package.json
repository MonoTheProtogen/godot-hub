{
  "name": "godot-hub",
  "version": "0.0.1",
  "description": "A simple project and version manager for the Godot game engine written with Electron",
  "main": "main.js",
  "scripts": {
    "test": "electron .",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "builder build": "electron-builder build",
    "builder start": "elecrton-builder start",
    "builder pack": "electron-builder --dir",
    "builder": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [
    "godot",
    "electron"
  ],
  "author": "MonoTheProtogen",
  "license": "GPL-3.0",
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron-forge/plugin-fuses": "^7.4.0",
    "@electron/fuses": "^1.8.0",
    "electron": "^31.3.1",
    "electron-builder": "^24.13.3"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1"
  }
}
